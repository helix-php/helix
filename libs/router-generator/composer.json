{
    "name": "helix/router-generator",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "psr/http-factory": "^1.0",
        "helix/router-contracts": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Router\\Generator\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "vimeo/psalm": "^4.22"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\Router\\Generator\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
