{
    "name": "helix/router",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "psr/http-message": "^1.0",
        "nikic/fast-route": "^1.3",
        "helix/router-generator": "^1.0",
        "helix/http-method": "^1.0",
        "helix/http-status-code": "^1.0",
        "helix/middleware-contracts": "^1.0",
        "helix/router-contracts": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Router\\": "src"
        }
    },
    "require-dev": {
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpunit/phpunit": "^9.3",
        "httpsoft/http-message": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\Router\\Tests\\": "tests"
        }
    },
    "provide": {
        "helix/router-contracts-implementation": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
